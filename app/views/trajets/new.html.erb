		<li class="active">Data</li>
	</ol>
	<!--/Breadcrumbs-->
	<!-- pabel content -->
	<div class="panel panel-default">
		<div class="panel-heading"><h3>Ajouter Trajet</h3></div>
		<div class="panel-body" >
			<!--img src="../logo.png" class="img-responsive img-rounded" alt="Responsive image" style="width: 100%;height: 369px;"-->
			<div class="jumbotron" align="center">
				<%= form_tag :action => 'create' do %>




				<div class="form-group">
					<label for = "conducteur">Conducteur</label>
					<%= collection_select(:trajets, :id_conducteur, @membres, :id, :nom, prompt: true)
					%>
				</div>



				<div class="form-group">
					<label for = "voiture">Voiture</label>:
					<%= collection_select(:trajets, :id_voiture, @voitures, :id, :marque, prompt: true)
					%>
				</div>


				<div class="form-group">
					<label for = "ville_source">Ville source</label>:
					<%= collection_select(:trajets, :id_ville_source, @villes, :id, :nom_ville, prompt: true)
					%>
				</div>




				<div class="form-group">
					<label for = "ville_destination">Ville destination</label>
					<%= collection_select(:trajets, :id_ville_destination, @villes, :id, :nom_ville, prompt: true)
					%>
				</div>


				<div class="form-group">
					<label for = "book_description">date</label>
					<%= text_field 'trajets', 'date' ,class:'form-control' %><br>
				</div>


				<div class="form-group">
					<label for = "book_description">nombre de place</label>
					<%= text_field 'trajets', 'nbr_place' ,class:'form-control' %>
				</div>

				<div class="form-group">
					<label for = "book_description">prix</label>
					<%= text_field 'trajets', 'prix' ,class:'form-control' %>     
				</div>

				<%= submit_tag "Create" %>
				<% end -%>
				<%= link_to 'Back', {:action => 'index'} %>


				
			</div>


		</div>
	</div>


</div>
